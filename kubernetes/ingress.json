{
  "apiVersion": "extensions/v1beta1",
  "kind": "Ingress",
  "metadata": {
    "name": "okno-proxy",
    "namespace": "default",
    "annotations": {
      "kubernetes.io/ingress.class": "nginx",
      "nginx.ingress.kubernetes.io/rewrite-target": "/"
    }
  },
  "spec": {
    "rules": [
      {
        "host": "auth.okno.pl",
        "http": {
          "paths": [
            {
              "path": null,
              "backend": {
                "serviceName": "okno-auth",
                "servicePort": 6666
              }
            }
          ]
        }
      },
      {
        "host": "svc.okno.pl",
        "http": {
          "paths": [
            {
              "path": null,
              "backend": {
                "serviceName": "okno-svc",
                "servicePort": 7777
              }
            }
          ]
        }
      }
    ]
  }
}